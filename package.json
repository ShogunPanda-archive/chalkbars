{
  "name": "chalkbars",
  "version": "1.1.0",
  "description": "Template based terminal coloring based on Chalk and Handlebars.",
  "main": "main.js",
  "files": [
    "main.js",
    "lib",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "postpublish": "git tag -f v-$npm_package_version && git push origin --tags && git push origin",
    "test": "./node_modules/.bin/mocha",
    "posttest": "./node_modules/.bin/eslint main.js lib/*.js test/*.js",
    "coverage": "./node_modules/.bin/mocha --require blanket -R html-cov > coverage.html; true",
    "travis-ci": "./node_modules/.bin/mocha --require blanket -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js; true",
    "docs": "./node_modules/.bin/jsdoc -c .jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -r"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShogunPanda/chalkbars"
  },
  "keywords": [
    "chalk",
    "handlebars",
    "terminal",
    "colors"
  ],
  "author": "Shogun <shogun@cowtech.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShogunPanda/chalkbars/issues"
  },
  "homepage": "https://sw.cowtech.it/chalkbars",
  "dependencies": {
    "chalk": "~1.1.1",
    "handlebars": "~4.0.5"
  },
  "devDependencies": {
    "blanket": "~1.2.3",
    "coveralls": "~2.11.8",
    "eslint": "~2.4.0",
    "expecting": "~0.1.0",
    "jsdoc": "~3.4.0",
    "ink-docstrap": "~1.1.4",
    "mocha": "~2.4.5",
    "mocha-lcov-reporter": "~1.2.0",
    "sinon": "~1.17.3"
  },
  "config": {
    "blanket": {
      "pattern": [
        "main.js",
        "lib"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
